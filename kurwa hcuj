/*
 {
    get {bool a = true;bool b = true; a = !a; b = !b; return (a , b);}   może działa
    } */



public static class Kata
{
  public static bool IsPrime(int n)
  {
     if(n <= 0){
   return false;  
 } 
else if(n == 1){
   return false; 
}
  for(int i=2;i*i<=n;i++){   
    if(n%i == 0){return false;}   
  }
  return true;
  }
}
